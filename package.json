{
  "name": "festival-site",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "cross-env IONOS_STATIC=true next build",
    "build:ionos": "pnpm build:static",
    "start": "next start",
    "start:static": "serve out",
    "lint": "next lint",
    "lint:strict": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit --strict",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next out node_modules pnpm-lock.yaml",
    "fresh-install": "pnpm clean && pnpm install",
    "ionos:deploy": "pnpm build:ionos"
  },
  "dependencies": {
    "@storyblok/react": "^5.4.4",
    "framer-motion": "^11.11.17",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storyblok-rich-text-react-renderer": "^3.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.1",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}